{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": ["transform-runtime"],
  "env": {
    "server": {
      "plugins": [
        ["css-modules-transform", {
          "generateScopedName": "[name]__[local]--[hash:base64:5]",
          "extensions": [".scss"]
        }
        ]
      ]
    }
  }
}