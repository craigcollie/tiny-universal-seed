{
  "name": "react-webpack2-flow-seed",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run clean && npm run dist && npm run serve:prod",
    "dev": "concurrently \"npm run client\" \"npm run serve:dev\"",
    "client": "node -r babel-register node_modules/.bin/webpack-dev-server --config=./webpack/webpack.config.dev --watch",
    "serve:dev": "babel-node ./src/server",
    "serve:prod": "NODE_ENV=production babel-node ./src/server",
    "dist": "node -r babel-register node_modules/.bin/webpack -p --config=./webpack/webpack.config.prod",
    "clean": "rm -rf public/*.js"
  },
  "dependencies": {
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "tiny-universal": "^0.0.24",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "6.8.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "^6.24.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "script-loader": "^0.7.0",
    "webpack-dev-server": "^2.4.2"
  }
}
